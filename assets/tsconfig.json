{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"verbatimModuleSyntax": true,
		"types": ["node"],
		"baseUrl": ".",
		"paths": {
			"$lib": ["svelte"],
			"$lib/*": ["svelte/*"]
		}
	}
}
